{
  "name": "transpile-preview",
  "version": "0.1.0",
  "description": "Quickly preview your transpiled source code in Atom.",
  "keywords": [
    "transpile",
    "convert",
    "refactor",
    "source",
    "preview"
  ],
  "homepage": "https://atom.io/packages/transpile",
  "repository": "danielbayley/atom-transpile",
  "bugs": "https://github.com/danielbayley/atom-transpile/issues",
  "author": "Daniel Bayley <daniel.bayley@me.com> (https://github.com/danielbayley)",
  "contributors": [
    "aki77 (https://github.com/aki77)"
  ],
  "private": true,
  "license": "MIT",
  "dependencies": {
    "loophole": "^1.1.0",
    "source-map": "^0.5.3",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "preinstall": "apm install transpile"
  },
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "providedServices": {
    "transpile_preview": {
      "versions": {
        "1.0.0": "activate"
      }
    }
  },
  "configSchema": {
    "scrollSync": {
      "type": "boolean",
      "default": true
    },
    "pinPreviewPane": {
      "description": "Leave preview pane open when switching tabs.",
      "type": "boolean",
      "default": false
    }
  },
  "projectmanager": {
    "title": "transpile_preview",
    "template": "atom-package"
  }
}
